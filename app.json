{
    "name": "aps-revit-dwg-export-bim360",
    "description": "This sample demonstrated how to export Revit views to DWGs using Design Automation API for Revit.",
    "repository": "https://github.com/yiskang/aps-revit-dwg-export-bim360",
    "logo": "https://avatars0.githubusercontent.com/u/8017462?v=3&s=200",
    "keywords": [
        "autodesk",
        "aps",
        "autodesk platform service",
        "design automation"        
    ],
    "env": {
        "APS_CLIENT_ID": {
            "description": "APS Client ID"
        },
        "APS_CLIENT_SECRET": {
            "description": "APS Client Secret"
        },
        "APS_CALLBACK_URL": {
            "description": "Callback URL of your APS app, required for 3-legged OAuth",
            "value": "https://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/api/aps/callback/oauth"
        },
        "APS_WEBHOOK_URL": {
            "description": "Callback URL of your webhook",
            "value": "http://<<YOUR HEROKU APP NAME HERE>>.herokuapp.com/api/aps/callback/designautomation"
        },
        "DESIGN_AUTOMATION_NICKNAME": {
            "description": "Revit Design Automation NickName, APS_CLIENT_ID by default",
            "value": "<<YOUR APS_CLIENT_ID HERE>>"
        },
        "DESIGN_AUTOMATION_ACTIVITY_NAME": {
            "description": "Revit Design Automation Activity Name",
            "value": "DWGExporterActivity+dev"
        }
    },
    "website": "https://developer.autodesk.com/",
    "success_url": "/"
}